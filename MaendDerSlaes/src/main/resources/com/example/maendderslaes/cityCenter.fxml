<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Button?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.StackPane?>

<StackPane xmlns:fx="http://javafx.com/fxml"
           fx:controller="com.example.maendderslaes.Controller">

    <VBox alignment="CENTER" spacing="20.0"
          style="-fx-background-image: url('file:data/images/CITYCENTER.png');
                 -fx-background-repeat: no-repeat;
                 -fx-background-size: cover;
                 -fx-background-position: center center;">

        <!-- Tilbage-knap i top-venstre hjÃ¸rne -->
        <VBox alignment="BOTTOM_RIGHT">
            <padding>
                <Insets top="900" left="20" right="20"/>
            </padding>
            <Button onAction="#goBack" text="Return to Character Creation"
                    style="-fx-background-color: #FFD700;
                           -fx-text-fill: #000000;
                           -fx-font-family: 'Comic Sans MS';
                           -fx-font-size: 14;
                           -fx-font-weight: bold;
                           -fx-effect: dropshadow(three-pass-box, rgba(0,0,0,0.6), 5, 0, 0, 0);
                           -fx-min-width: 100;
                           -fx-min-height: 30;"/>
        </VBox>
    </VBox>
</StackPane>