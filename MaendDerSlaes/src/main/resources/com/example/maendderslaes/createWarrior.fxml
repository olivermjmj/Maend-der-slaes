<?xml version="1.0" encoding="UTF-8"?>

        <?import javafx.scene.control.Button?>
        <?import javafx.scene.layout.AnchorPane?>

<?import javafx.scene.control.TextField?>
<?import javafx.scene.text.Text?>
<?import javafx.scene.control.Label?>
<AnchorPane xmlns:fx="[http://javafx.com/fxml](http://javafx.com/fxml)" fx:controller="com.example.maendderslaes.Controller" style="-fx-background-image: url('file:data/images/SkabDinKrigerScene.png'); -fx-background-repeat: no-repeat; -fx-background-size: cover; -fx-background-position: center center; -fx-background-color: black;">

    <Text
            text="Skill points left: "
            style="-fx-background-color: #FFD700;
               -fx-text-fill: #000000;
               -fx-font-family: 'Comic Sans MS';
               -fx-font-size: 18;
               -fx-font-weight: bold;
               -fx-min-height: 30;
               -fx-min-width: 200;
               -fx-effect: dropshadow(three-pass-box, rgba(255,215,0,0.8), 15, 0.5, 0, 0);
               -fx-border-color: #000000;
               -fx-border-width: 2;
               -fx-background-radius: 5;
               -fx-border-radius: 5;"
            AnchorPane.rightAnchor="1100.0"
            AnchorPane.bottomAnchor="780.0"/>

    <Text fx:id="skillPointsLeft"
            text="10"
            style="-fx-background-color: #FFD700;
               -fx-text-fill: #000000;
               -fx-font-family: 'Comic Sans MS';
               -fx-font-size: 18;
               -fx-font-weight: bold;
               -fx-min-height: 30;
               -fx-min-width: 200;
               -fx-effect: dropshadow(three-pass-box, rgba(255,215,0,0.8), 15, 0.5, 0, 0);
               -fx-border-color: #000000;
               -fx-border-width: 2;
               -fx-background-radius: 5;
               -fx-border-radius: 5;"
            AnchorPane.rightAnchor="1100.0"
            AnchorPane.bottomAnchor="780.0"/>

    <Text
            text="Strength"
            style="-fx-background-color: #FFD700;
               -fx-text-fill: #000000;
               -fx-font-family: 'Comic Sans MS';
               -fx-font-size: 18;
               -fx-font-weight: bold;
               -fx-min-height: 30;
               -fx-min-width: 200;
               -fx-effect: dropshadow(three-pass-box, rgba(255,215,0,0.8), 15, 0.5, 0, 0);
               -fx-border-color: #000000;
               -fx-border-width: 2;
               -fx-background-radius: 5;
               -fx-border-radius: 5;"
            AnchorPane.rightAnchor="1100.0"
            AnchorPane.bottomAnchor="780.0"/>

    <Text fx:id="strengthLevel"
            text="1"
            style="-fx-background-color: #FFD700;
               -fx-text-fill: #000000;
               -fx-font-family: 'Comic Sans MS';
               -fx-font-size: 18;
               -fx-font-weight: bold;
               -fx-min-height: 30;
               -fx-min-width: 200;
               -fx-effect: dropshadow(three-pass-box, rgba(255,215,0,0.8), 15, 0.5, 0, 0);
               -fx-border-color: #000000;
               -fx-border-width: 2;
               -fx-background-radius: 5;
               -fx-border-radius: 5;"
            AnchorPane.rightAnchor="1100.0"
            AnchorPane.bottomAnchor="780.0"/>

    <Button onAction="#addStrength"
            text="+"
            style="-fx-background-color: #FFD700;
               -fx-cursor: hand;
               -fx-text-fill: #000000;
               -fx-font-family: 'Comic Sans MS';
               -fx-font-size: 18;
               -fx-font-weight: bold;
               -fx-min-height: 30;
               -fx-min-width: 200;
               -fx-effect: dropshadow(three-pass-box, rgba(255,215,0,0.8), 15, 0.5, 0, 0);
               -fx-border-color: #000000;
               -fx-border-width: 2;
               -fx-background-radius: 5;
               -fx-border-radius: 5;"
            AnchorPane.rightAnchor="525.0"
            AnchorPane.bottomAnchor="100.0"/>

    <Button onAction="#negateStrength"
            text="-"
            style="-fx-background-color: #FFD700;
               -fx-cursor: hand;
               -fx-text-fill: #000000;
               -fx-font-family: 'Comic Sans MS';
               -fx-font-size: 18;
               -fx-font-weight: bold;
               -fx-min-height: 30;
               -fx-min-width: 200;
               -fx-effect: dropshadow(three-pass-box, rgba(255,215,0,0.8), 15, 0.5, 0, 0);
               -fx-border-color: #000000;
               -fx-border-width: 2;
               -fx-background-radius: 5;
               -fx-border-radius: 5;"
            AnchorPane.rightAnchor="525.0"
            AnchorPane.bottomAnchor="100.0"/>


    <Button onAction="#goBack"
            text="GO BACK"
            style="-fx-background-color: #FFD700;
               -fx-cursor: hand;
               -fx-text-fill: #000000;
               -fx-font-family: 'Comic Sans MS';
               -fx-font-size: 15;
               -fx-font-weight: bold;
               -fx-effect: dropshadow(three-pass-box, rgba(0,0,0,0.6), 5, 0, 0, 0);
               -fx-min-width: 70;
               -fx-min-height: 30;"
            AnchorPane.rightAnchor="50.0"
            AnchorPane.bottomAnchor="100.0"/>

    <Button onAction="#goToCityCenter"
            text="GO TO CITY CENTER"
            style="-fx-background-color: #FFD700;
               -fx-cursor: hand;
               -fx-text-fill: #000000;
               -fx-font-family: 'Comic Sans MS';
               -fx-font-size: 18;
               -fx-font-weight: bold;
               -fx-min-height: 30;
               -fx-min-width: 200;
               -fx-effect: dropshadow(three-pass-box, rgba(255,215,0,0.8), 15, 0.5, 0, 0);
               -fx-border-color: #000000;
               -fx-border-width: 2;
               -fx-background-radius: 5;
               -fx-border-radius: 5;"
            AnchorPane.rightAnchor="525.0"
            AnchorPane.bottomAnchor="100.0"/>
</AnchorPane>