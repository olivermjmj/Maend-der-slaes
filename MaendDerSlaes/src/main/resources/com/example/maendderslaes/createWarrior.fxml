<?xml version="1.0" encoding="UTF-8"?>

        <?import javafx.scene.control.Button?>
        <?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns:fx="[http://javafx.com/fxml](http://javafx.com/fxml)" fx:controller="com.example.maendderslaes.Controller" style="-fx-background-image: url('file:data/images/SkabDinKrigerScene.png'); -fx-background-repeat: no-repeat; -fx-background-size: cover; -fx-background-position: center center; -fx-background-color: black;">
    <Button onAction="#goBack"
            text="Return to MainMenu"
            style="-fx-background-color: #FFD700;
               -fx-text-fill: #000000;
               -fx-font-family: 'Comic Sans MS';
               -fx-font-size: 14;
               -fx-font-weight: bold;
               -fx-effect: dropshadow(three-pass-box, rgba(0,0,0,0.6), 5, 0, 0, 0);
               -fx-min-width: 100;
               -fx-min-height: 30;"
            AnchorPane.rightAnchor="150.0"
            AnchorPane.bottomAnchor="20.0"/>

    <Button onAction="#goToCityCenter"
            text="GO TO CITY CENTER"
            style="-fx-background-color: #FFD700;
               -fx-text-fill: #000000;
               -fx-font-family: 'Comic Sans MS';
               -fx-font-size: 18;
               -fx-font-weight: bold;
               -fx-min-height: 40;
               -fx-min-width: 200;
               -fx-effect: dropshadow(three-pass-box, rgba(255,215,0,0.8), 15, 0.5, 0, 0);
               -fx-border-color: #000000;
               -fx-border-width: 2;
               -fx-background-radius: 5;
               -fx-border-radius: 5;"
            AnchorPane.rightAnchor="525.0"
            AnchorPane.bottomAnchor="30.0"/>
</AnchorPane>